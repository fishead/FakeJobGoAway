{
   "manifest_version": 2,
   "name": "Fake job go away",
   "version": "1.0.0",
   "description": "搜索工作机会时看到满屏培训机构发布的信息感到沮丧，马上安装我，让烦人的信息视而不见！",
   "icons": {
      "16": "icon_default_128x128.png",
      "48": "icon_default_128x128.png",
      "128": "icon_default_128x128.png"
   },
   "browser_action": {
      "default_title": "Fake job go away",
      "default_icon": "icon_default_128x128.png",
      "default_badge": "",
      "default_popup": "popup.html"
   },
   "background": {
      "persistent": false,
      "scripts": ["js/background.js"]
   },
   "options_page": "options.html",
   "permissions": [
      "tabs",
      "storage"
   ],
   "content_scripts": [ 
      {
         "matches": ["*://*.58.com/*"],
         "js": [
            "lib/jquery.min.js",
            "js/58.js"
         ]         
      }, 
      {
         "matches": ["*://*.ganji.com/*"],
         "js": [
            "lib/jquery.min.js",
            "js/ganji.js"
         ]         
      },
      {
         "matches": ["*://*.zhaopin.com/*"],
         "js": [
            "lib/jquery.min.js",
            "js/zhaopin.js"
         ]
      },
      {
         "matches": ["*://*.51job.com/*"],
         "js": [
            "lib/jquery.min.js",
            "js/51job.js"
         ]
      }
   ],
   "web_accessible_resources": [
      "lib/jquery.min.map"
   ],
   "update_url": "http://clients2.google.com/service/update2/crx"
}
